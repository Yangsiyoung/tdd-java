

<!--  요별 웹툰, DB 구조, 요일별 웹툰 SELECT-->

CREATE TABLE WEB_TOON
ID
TITLE



CREATE TABLE WEB_TOON_BY_DAY
WEB_TOON_ID
DAY


@Enumurate
private Day day;

public enum Day {
    MONDAY,
}

MON, TUE, WED

SELECT DISTINCT id, title
FROM WEB_TOON as webtoon , WEB_TOON_BY_DAY as day
WHERE
        webtoon.ID = day.WEB_TOON_ID
        AND day.DAY in (#{day})
